            {reduce,    "steps"},
            {format,    "0,0"}
           ]}
         ]},
        {col, [{class, "col-md-3"}],
         [
          {"value-panel",
           [{color,      "green"},
            {icon,       "clock-o"},
            {caption,    "Time"},
            {source,     "summary"},
            {attribute,  "time"},
            {format,     "00:00:00"}
           ]}
         ]},
        {col, [{class, "col-md-3"}],
         [
          {"value-panel",
           [{color,      "blue-600"},
            {icon,       "dashboard"},
            {caption,    "Avg System CPU"},
            {source,     "series/sys/cpu/util"},
            {reduce,     "average"},
            {format,     "0.0%"}
           ]}
         ]},
        {col, [{class, "col-md-3"}],
         [
          {"value-panel",
           [{color,      "blue-600"},
            {icon,       "question-circle"},
            {caption,    "Avg Test Random"},
            {source,     "series/test/rand"},
            {reduce,     "average"},
            {format,     "0.0"}
           ]}
         ]}
       ]},
      {row,
       [
        {col, [{class, "col-md-6"}],
         [
          {"timeseries",
           [{title, "Process Memory"},
            {label, "RSS"},
            {format, "0.0 b"},
            {source, "series/op/mem/rss"}
           ]}
         ]},
        {col, [{class, "col-md-6"}],
         [
          {"timeseries",
           [{title, "Process CPU"},
            {label, "CPU % Util"},
            {format, "%"},
            {source, "series/op/cpu/util"}
           ]}
         ]},
        {col, [{class, "col-md-6"}],
         [
          {"timeseries",
           [{title, "System Memory"},
            {label, "Mem Used"},
            {format, "0.0 b"},
            {source, "series/sys/mem/used"}
