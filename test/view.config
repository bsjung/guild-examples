        {col, [{class, "col-md-6"}],
         [
          {"timeseries",
           [{title, "System CPU"},
            {label, "CPU % Util"},
            {format, "%"},
            {source, "series/sys/cpu/util"}
           ]}
         ]},
        {col, [{class, "col-md-6"}],
         [
          {"timeseries",
           [{title, "System Disk % Util"},
            {label, "Disk % Util"},
