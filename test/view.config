            {source, "series/sys/dev.*/util"}
           ]}
         ]},
        {col, [{class, "col-md-6"}],
         [
          {"timeseries",
           [{title, "System Memory/Swap % Util"},
            {label, "Mem % Util"},
            {format, "%"},
            {source, "series/sys/(mem|swap)/util"}
