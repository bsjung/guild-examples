%%-*-erlang-*-
{rows,
 [{row,
   [{col, [{class, "col-md-9"}],
     [
      {row,
       [
        {col, [{class, "col-md-3"}],
         [
          {"value-panel",
           [{color,   "green"},
            {icon,    "bullseye"},
            {caption, "Precision @ 1"},
            {source,  "series/tf/test/Precision @ 1"},
            {reduce,  "last"},
            {format,  "0.0%"}
           ]}
         ]},
        {col, [{class, "col-md-3"}],
         [
          {"value-panel",
           [{color,   "red"},
            {icon,    "random"},
            {caption, "Total Loss"},
            {source,  "series/tf/train/total_loss"},
            {reduce,  "last"},
            {format,  "0.00"}
           ]}
         ]},
        {col, [{class, "col-md-3"}],
         [
          {"value-panel",
           [{color,   "blue-600"},
            {icon,    "retweet"},
            {caption, "Steps"},
            {source,  "series/tf/train/total_loss"},
            {reduce,  "steps0"},
            {format,  "0,0"}
           ]}
         ]},
        {col, [{class, "col-md-3"}],
         [
          {"value-panel",
           [{color,   "yellow-600"},
            {icon,    "clock-o"},
            {caption, "Time"},
            {source,  "series/tf/train/total_loss"},
            {reduce,  "duration"},
            {format,  "00:00:00"}
           ]}
         ]},
        {col, [{class, "col-md-6"}],
         [
          {"timeseries",
           [{title, "Total Loss"},
            {label, "Cross Entropy"},
            {format, "0.00"},
            {source, "series/tf/train/total_loss"}
           ]}
         ]
        },
        {col, [{class, "col-md-6"}],
         [
          {"timeseries",
           [{title, "Precision"},
            {label, "% Correct"},
            {format, "0.00%"},
            {source, "series/tf/test/Precision.*"}
           ]}
         ]
        },
        {col, [{class, "col-md-4"}],
         [
          {"timeseries",
           [{title, "Learning Rate"},
            {label, ""},
            {format, "0.0"},
            {source, "series/tf/train/learning_rate"}
           ]}
         ]
        },
        {col, [{class, "col-md-4"}],
         [
          {"timeseries",
           [{title, "GPU Memory"},
            {label, "Memory"},
            {format, "0.0 b"},
            {source, "series/sys/gpu.*/mem/used"}
           ]}
         ]},
        {col, [{class, "col-md-4"}],
         [
          {"timeseries",
           [{title, "GPU Util"},
            {label, "GPU % Util"},
            {format, "%"},
            {source, "series/sys/gpu.*/gpu/util"}
           ]}
         ]},
        {col, [{class, "col-md-4"}],
         [
          {"timeseries",
           [{title, "GPU Temp"},
            {label, "Degress C"},
            {format, "0.0"},
            {source, "series/sys/gpu.*/temp"}
           ]}
         ]},
        {col, [{class, "col-md-4"}],
         [
          {"timeseries",
           [{title, "GPU Power Draw"},
            {label, "Watts"},
            {format, "0.0"},
            {source, "series/sys/gpu.*/powerdraw"}
           ]}
         ]},
        {col, [{class, "col-md-4"}],
         [
          {"timeseries",
           [{title, "Process CPU Memory"},
            {label, "Memory (RSS)"},
            {format, "0.0 b"},
            {source, "series/op/mem/rss"}
           ]}
         ]},
        {col, [{class, "col-md-4"}],
         [
          {"timeseries",
           [{title, "Process CPU Util"},
            {label, "CPU % Util"},
            {format, "%"},
            {source, "series/op/cpu/util"}
           ]}
         ]},
        {col, [{class, "col-md-4"}],
         [
          {"timeseries",
           [{title, "System Memory"},
            {label, "Mem % Util"},
            {format, "%"},
            {source, "series/sys/(mem|swap)/util"}
           ]}
         ]},
        {col, [{class, "col-md-4"}],
         [
          {"timeseries",
           [{title, "System CPU Util"},
            {label, "CPU % Util"},
            {format, "%"},
            {source, "series/sys/cpu.+/util"}
           ]}
         ]},
        {col, [{class, "col-md-4"}],
         [
          {"timeseries",
           [{title, "Disk Util"},
            {label, "Disk % Util"},
            {format, "%"},
            {source, "series/sys/dev.*/util"}
           ]}
         ]}
       ]},
      {row,
       [{col, [{class, "col-md-12"}],
         [
          {"output", []}
         ]}
       ]}
     ]},
    {col, [{class, "col-md-3"}],
     [
      {"status", []},
      {"flags", []}
     ]}
   ]}
 ]}.
